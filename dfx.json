{
  "canisters": {
    "backend": {
      "candid": "src/backend/backend.did",
      "declarations": {
        "node_compatibility": true
      },
      "package": "backend",
      "build": [
        "cargo build --release -p backend --target wasm32-wasip1",
        "wasi2ic target/wasm32-wasip1/release/backend.wasm target/wasm32-wasip1/release/nowasi.wasm",
        "ic-wasm -o target/wasm32-wasip1/release/shrinked.wasm target/wasm32-wasip1/release/nowasi.wasm shrink",
        "gzip -f target/wasm32-wasip1/release/shrinked.wasm"
      ],
      "wasm": "target/wasm32-wasip1/release/shrinked.wasm.gz",
      "type": "custom"
    },
    "frontend": {
      "dependencies": [
        "backend"
      ],
      "source": [
        "src/frontend/dist"
      ],
      "type": "assets",
      "workspace": "frontend"
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "output_env_file": ".env",
  "version": 1
}